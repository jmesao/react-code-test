    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "@vitejs/plugin-react": "^2.1.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
